{
  "name": "flauncher",
  "version": "2.3.6",
  "productName": "FLauncher",
  "description": "Modded Minecraft Launcher",
  "author": "Nikita Gutsenkov (https://github.com/Envel-Experimental/)",
  "license": "UNLICENSED",
  "homepage": "https://github.com/Envel-Experimental/HeliosLauncher",
  "bugs": {
    "url": "https://github.com/Envel-Experimental/HeliosLauncher/issues"
  },
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder build",
    "dist:win": "npm run dist -- -w",
    "dist:mac": "npm run dist -- -m",
    "dist:linux": "npm run dist -- -l",
    "lint": "eslint --config .eslintrc.json .",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:coverage": "jest --coverage",
    "test:performance": "xvfb-run --auto-servernum --server-args=\"-screen 0 1280x960x24\" -- playwright test tests/performance",
    "test:smoke": "xvfb-run --auto-servernum --server-args=\"-screen 0 1280x960x24\" -- playwright test tests/smoke"
  },
  "engines": {
    "node": ">=20.x.x"
  },
  "dependencies": {
    "@electron/remote": "^2.1.2",
    "@envel/helios-core": "2.3.10",
    "@sentry/electron": "^7.1.1",
    "@sentry/profiling-node": "^10.15.0",
    "adm-zip": "^0.5.16",
    "check-disk-space": "^3.4.0",
    "ejs": "^3.1.10",
    "ejs-electron": "^3.0.0",
    "electron-updater": "^6.3.9",
    "fs-extra": "^11.1.1",
    "github-syntax-dark": "^0.5.0",
    "got": "^11.8.5",
    "helios-distribution-types": "^1.3.0",
    "jquery": "^3.7.1",
    "lodash.merge": "^4.6.2",
    "semver": "^7.6.3",
    "toml": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.5",
    "@playwright/test": "^1.56.1",
    "babel-jest": "^30.2.0",
    "electron": "^38.1.2",
    "electron-builder": "^26.0.12",
    "eslint": "^8.57.1",
    "jest": "^30.2.0",
    "msw": "^2.11.6",
    "playwright": "^1.56.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Envel-Experimental/HeliosLauncher.git"
  }
}
