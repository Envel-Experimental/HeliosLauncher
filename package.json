{
  "name": "flauncher",
  "version": "2.4.0-beta",
  "productName": "FLauncher",
  "description": "Modded Launcher",
  "author": "Nikita Gutsenkov (https://github.com/Envel-Experimental/)",
  "license": "UNLICENSED",
  "homepage": "https://github.com/Envel-Experimental/HeliosLauncher",
  "bugs": {
    "url": "https://github.com/Envel-Experimental/HeliosLauncher/issues"
  },
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "postinstall": "patch-package",
    "dist": "electron-builder build",
    "dist:win": "npm run dist -- -w",
    "dist:mac": "npm run dist -- -m",
    "dist:linux": "npm run dist -- -l",
    "lint": "eslint --config .eslintrc.json .",
    "test": "jest && playwright test tests/smoke.spec.js",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:coverage": "jest --coverage",
    "test:performance": "xvfb-run --auto-servernum --server-args=\"-screen 0 1280x960x24\" -- playwright test tests/performance",
    "test:smoke": "xvfb-run --auto-servernum --server-args=\"-screen 0 1280x960x24\" -- playwright test tests/smoke.spec.js"
  },
  "engines": {
    "node": ">=20.x.x"
  },
  "dependencies": {
    "@electron/remote": "^2.1.3",
    "@sentry/electron": "^7.7.1",
    "@sentry/profiling-node": "^10.39.0",
    "b4a": "^1.7.5",
    "ejs": "^4.0.1",
    "ejs-electron": "^3.0.0",
    "electron-updater": "^6.7.3",
    "helios-distribution-types": "^1.3.0",
    "hyperdht": "^6.29.0",
    "hyperswarm": "^4.16.0",
    "p-limit": "^7.3.0",
    "semver": "^7.7.4",
    "smol-toml": "^1.6.0",
    "udx-native": "^1.19.2"
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@babel/preset-env": "^7.29.0",
    "@playwright/test": "^1.58.2",
    "ajv-formats": "^3.0.1",
    "babel-jest": "^30.2.0",
    "baseline-browser-mapping": "^2.9.19",
    "electron": "^40.4.1",
    "electron-builder": "^26.7.0",
    "eslint": "^10.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "msw": "^2.12.10",
    "patch-package": "^8.0.1",
    "playwright": "^1.58.2"
  },
  "overrides": {
    "ajv": "^8.17.1",
    "ajv-keywords": "^5.1.0",
    "js-yaml": "^4.1.1",
    "tar": "^7.5.3",
    "@develar/schema-utils": {
      "ajv": "^8.17.1",
      "ajv-keywords": "^5.1.0"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Envel-Experimental/HeliosLauncher.git"
  }
}